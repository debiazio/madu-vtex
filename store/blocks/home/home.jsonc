        {
          "mobileImage": "assets/banners/mobile/01-Banner-mobile-Restaurante.webp",
          "image": "assets/banners/desktop/Banner-desktop-Restaurante-scaled.webp",
          "link": {
            "url": "/moveis/carrinhos"
          }
        },
        {
          "mobileImage": "assets/banners/mobile/02-Banner-mobile-Escritorio.webp",
          "image": "assets/banners/desktop/02-Banner-desktop-Escritorio-scaled.webp",
