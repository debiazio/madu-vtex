          "link": {
            "url": "/produtos/carrinho-de-cafe-e-bebidas-multiuso-mad-u-one-740/"
          }
        }
      ],
      "title": "Banner home"
    }
  },
  "slider-layout#home": {
    "props": {
      "title": "Slider home",
      "blockClass": "slider-home-desktop",
      "maxwidth": "1200",
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "arrowSize": 50,
      "infinite": true,
      "showNavigationArrows": "always",
      "showPaginationDots": "never",
      "autoplay": {
        "timeout": 8000,
        "stopOnHover": true
      }
    }
  },

  "list-context.product-list#mais-vendidos": {
    "blocks": [
