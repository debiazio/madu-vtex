    }
  },
  "image#logo-madubox": {
    "props": {
      "src": "assets/madu-box-logo.webp",
      "alt": "Logo Madubox",
      "blockClass": "logo-madubox"
    }
  },
  "image#interrogacao": {
    "props": {
      "src": "assets/interrogacao.webp",
      "alt": "Interrogação",
      "blockClass": "interrogacao"
    }
